<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform"
               xmlns:nlg="http://www.nlog-project.org/schemas/NLog.xsd">
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>

  <connectionStrings xdt:Transform="RemoveAttributes(configSource)">
    <remove name="eSrvNew" xdt:Transform="Insert" />
    <add name="eSrvNew" connectionString="Data Source=ESSQL01;Initial Catalog=eSrv_Assm;Integrated Security=True;Connect Timeout=10;Encrypt=False;Network Library=dbmssocn;Application Name=eSrvApp" providerName="System.Data.SqlClient" xdt:Transform="Insert" />
    <add name="eServicesSessionStateStore" connectionString="Data Source=devsql03;Initial Catalog=EServices_AspStateDb;Integrated Security=True;Connect Timeout=10;Encrypt=False;Network Library=dbmssocn;Application Name=eServices" providerName="System.Data.SqlClient" xdt:Transform="Insert" />
  </connectionStrings>
  
  <appSettings>
    <add key="UmaConnUrl:Uma.Eservices.UmaConnContracts.IUmaMasterDataService" value="net.tcp://localhost:809/asmtest_main/UmaMasterDataService.svc" xdt:Transform="SetAttributes(value)" xdt:Locator="Match(key)" />
  </appSettings>
  
  <nlg:nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xdt:Transform="RemoveAttributes(internalLogLevel,internalLogFile)">
    <nlg:targets>
      <nlg:target name="asyncDebugWrapperTarget" xdt:Transform="Remove" xdt:Locator="Match(name)" />
    </nlg:targets>
    <nlg:rules>
      <nlg:logger xdt:Transform="RemoveAll" />
      <nlg:logger name="*" minlevel="Info" maxlevel="Fatal" writeTo="asyncEventLogWrapperTarget" xdt:Transform="Insert" />
    </nlg:rules>
  </nlg:nlog>

</configuration>